{
  "name": "feed",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint ./src/ ./__tests__/",
    "lint-fix": "eslint --fix ./src/ ./__tests__/",
    "generate-coverage": "jest --coverage --silent --noStackTrace || true",
    "update-coverage": "npm run generate-coverage && codeclimate-test-reporter < ./coverage/lcov.info",
    "update-snapshot": "jest -u -t snapshot",
    "test-watch": "jest --watch",
    "test-ci": "jest",
    "test": "jest"
  },
  "dependencies": {
    "@expo/ex-navigation": "^2.11.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-admob": "^1.3.0",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-message-bar": "^1.6.0",
    "react-native-scrollable-tab-view": "^0.6.6",
    "react-native-splash-screen": "^2.1.0",
    "react-native-vector-icons": "^4.1.1",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "codeclimate-test-reporter": "^0.5.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.4.0",
    "jest-expo": "^18.0.0",
    "prettier-eslint": "^6.4.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./__tests__/setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/**/**/*.test.js?(x)"
    ]
  }
}
